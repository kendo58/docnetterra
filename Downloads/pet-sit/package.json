{
  "name": "sitswap",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.8.3",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "build": "next build --webpack",
    "admin:dev": "next dev apps/admin --webpack",
    "admin:build": "next build apps/admin --webpack",
    "admin:start": "next start apps/admin",
    "dev": "next dev",
    "check:toolchain": "node scripts/check-toolchain.mjs",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings 0",
    "start": "node scripts/start-production.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm test && npm run test:scripts",
    "test:typecheck": "tsc -p tsconfig.tests.json --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:critical": "node scripts/assert-e2e-env.mjs && playwright test e2e/auth.spec.ts e2e/booking-payment.spec.ts",
    "test:e2e:local": "npm run build && npm run test:e2e",
    "check": "npm run check:toolchain && npm run lint && npm run test:typecheck && npm run test:all && npm run build && npm run admin:build",
    "check:ci": "npm run check:toolchain && npm run lint && npm run test:typecheck && npm run test:all && npm run build && npm run admin:build && npm run test:e2e",
    "smoke:migrations": "node scripts/post-migration-smoke-checks.mjs",
    "verify:split-deploy": "node scripts/verify-split-deploy.mjs",
    "loadtest:smoke": "node scripts/load-test.mjs --scenario smoke",
    "loadtest:api": "node scripts/load-test.mjs --scenario api",
    "backfill:geocodes": "tsx scripts/backfill-geocodes.ts",
    "admin:create": "tsx scripts/create-admin.ts",
    "test:scripts": "tsx scripts/tests/run-all-tests.ts",
    "test:scripts:db": "tsx scripts/tests/test-database-integration.ts",
    "test:scripts:db:required": "node scripts/assert-db-test-env.mjs && SITSWAP_REQUIRE_DB_TESTS=true tsx scripts/tests/test-database-integration.ts",
    "test:scripts:legacy": "tsx scripts/run-tests.ts",
    "worker": "tsx scripts/worker.ts"
  },
